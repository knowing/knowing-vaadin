<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataProcessingUnit name="iris-url-10fold-cross-validation">
	<description>Cross Validation with NaiveBayes output to TablePresenter. Input from URL: http://www.hakank.org/weka/iris.arff
	</description>
	<tags>Cross, Validation, Cross Validation, ARFF, NaiveBayes, Filter</tags>
	<nodes>
		<node nodeType="processor" factoryId="de.lmu.ifi.dbs.knowing.core.validation.XCrossValidator" id="validator">
			<properties>
				<item value="weka.classifiers.bayes.NaiveBayes" key="classifier" />
				<item value="10" key="folds" />

				<item value="false" key="kernel-estimator" />
				<item value="false" key="supervised-discretization" />
			</properties>
		</node>
		<node nodeType="loader" factoryId="weka.core.converters.ArffLoader"
			id="ARFF">
			<properties>
				<item value="true" key="absolute-path" />
				<item value="http://www.hakank.org/weka/iris.arff" key="url" />
			</properties>
		</node>
		<node nodeType="presenter" factoryId="de.lmu.ifi.dbs.knowing.vaadin.presenter.TablePresenter"
			id="TablePresenter">
			<properties>
				<item value="100" key="rows" />
			</properties>
		</node>
	</nodes>
	<edges>
		<edge weight="1" targetId="validator" sourceId="ARFF" id="arff_validator" />
		<edge weight="1" targetId="TablePresenter" sourceId="validator"	id="validator_presenter" />
	</edges>
</DataProcessingUnit>